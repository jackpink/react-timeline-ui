{
  "name": "react-timeline-ui",
  "version": "1.0.3",
  "description": "A selectable timeline UI component for use in React",
  "scripts": {
    "rollup": "rollup -c",
    "build-typescript": "rm -rf dist/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build-parcel": "rm -rf dist/ && parcel build"
  },
  "author": "Jack Pinkerton",
  "license": "ISC",
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.0",
    "@parcel/transformer-typescript-types": "^2.8.0",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@testing-library/react": "^13.4.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "parcel": "^2.8.0",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.4.19",
    "rollup": "^3.2.5",
    "rollup-plugin-dts": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackpink/react-timeline-ui.git"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "react": "^18.2.0"
  }
}
